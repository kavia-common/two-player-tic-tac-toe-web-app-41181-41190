# PUBLIC INTERFACE: Environment variables used by the frontend build/runtime.
# Copy to .env and fill in values appropriate for your environment.
# Do NOT commit your real .env file.

REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
# In CI, disable source maps to reduce memory usage during build
# Set to 'false' for CI to avoid OOM/exit 137: REACT_APP_ENABLE_SOURCE_MAPS=false
REACT_APP_PORT=3000
# If the CI environment has 3000 in use, change this to a free port (e.g., 5173)
# REACT_APP_PORT=5173
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
